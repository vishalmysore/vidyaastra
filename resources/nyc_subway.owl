<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:nycSubway="http://vidyaastra.org/ontology/nycSubway#">

  <owl:Ontology rdf:about="http://vidyaastra.org/ontology/nycSubway#">
    <rdfs:label>Sample NYC Subway Passenger Help System Ontology</rdfs:label>
    <rdfs:comment>An ontology representing NYC Subway stations, passenger services, accessibility features, emergency protocols, and fare information.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
  </owl:Ontology>

  <!-- ==================== CLASS DEFINITIONS ==================== -->

  <!-- Main Entity Classes -->
  <owl:Class rdf:about="http://vidyaastra.org/ontology/nycSubway#PassengerEntity">
    <rdfs:label>Passenger Entity</rdfs:label>
    <rdfs:comment>Base class for all entities in the NYC Subway passenger help system</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://vidyaastra.org/ontology/nycSubway#SubwayStation">
    <rdfs:label>Subway Station</rdfs:label>
    <rdfs:comment>A physical subway station in the NYC MTA network</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:Class>

  <owl:Class rdf:about="http://vidyaastra.org/ontology/nycSubway#FareService">
    <rdfs:label>Fare Service</rdfs:label>
    <rdfs:comment>Services related to subway fares and payment systems</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:Class>

  <owl:Class rdf:about="http://vidyaastra.org/ontology/nycSubway#AccessibilityService">
    <rdfs:label>Accessibility Service</rdfs:label>
    <rdfs:comment>Services and features related to accessibility for passengers with disabilities</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:Class>

  <owl:Class rdf:about="http://vidyaastra.org/ontology/nycSubway#SupportService">
    <rdfs:label>Support Service</rdfs:label>
    <rdfs:comment>General customer support and information services</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:Class>

  <owl:Class rdf:about="http://vidyaastra.org/ontology/nycSubway#BackendSystem">
    <rdfs:label>Backend System</rdfs:label>
    <rdfs:comment>System infrastructure supporting passenger services</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:Class>

  <owl:Class rdf:about="http://vidyaastra.org/ontology/nycSubway#EmergencyService">
    <rdfs:label>Emergency Service</rdfs:label>
    <rdfs:comment>Emergency and safety-related services</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:Class>

  <!-- ==================== PROPERTY DEFINITIONS ==================== -->

  <!-- Data Properties -->
  <owl:DatatypeProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#entityName">
    <rdfs:label>Entity Name</rdfs:label>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#description">
    <rdfs:label>Description</rdfs:label>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#isADAAccessible">
    <rdfs:label>Is ADA Accessible</rdfs:label>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment>Indicates whether the station is fully ADA accessible</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#hasElevators">
    <rdfs:label>Has Elevators</rdfs:label>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#supportPhoneNumber">
    <rdfs:label>Support Phone Number</rdfs:label>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#operatingHours">
    <rdfs:label>Operating Hours</rdfs:label>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#providesService">
    <rdfs:label>Provides Service</rdfs:label>
    <rdfs:comment>Relates an entity to the services it provides</rdfs:comment>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
    <rdfs:range rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#supportsAccessibility">
    <rdfs:label>Supports Accessibility</rdfs:label>
    <rdfs:comment>Relates a station to accessibility services available</rdfs:comment>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <rdfs:range rdf:resource="http://vidyaastra.org/ontology/nycSubway#AccessibilityService"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#acceptsFarePayment">
    <rdfs:label>Accepts Fare Payment</rdfs:label>
    <rdfs:comment>Relates a station to fare services it accepts</rdfs:comment>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <rdfs:range rdf:resource="http://vidyaastra.org/ontology/nycSubway#FareService"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#staffedBy">
    <rdfs:label>Staffed By</rdfs:label>
    <rdfs:comment>Relates a station to support services available</rdfs:comment>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <rdfs:range rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#canContactVia">
    <rdfs:label>Can Contact Via</rdfs:label>
    <rdfs:comment>Relates an entity to methods of contact</rdfs:comment>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
    <rdfs:range rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://vidyaastra.org/ontology/nycSubway#connectsTo">
    <rdfs:label>Connects To</rdfs:label>
    <rdfs:comment>Relates a station to services it connects passengers to</rdfs:comment>
    <rdfs:domain rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <rdfs:range rdf:resource="http://vidyaastra.org/ontology/nycSubway#PassengerEntity"/>
  </owl:ObjectProperty>

  <!-- ==================== INDIVIDUALS (INSTANCES) ==================== -->

  <!-- Subway Stations -->
  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#TimesSquare42St">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <nycSubway:entityName>Times Square–42 St</nycSubway:entityName>
    <nycSubway:description>Major hub: multiple lines, station agents, OMNY readers, some elevators/escalators</nycSubway:description>
    <nycSubway:isADAAccessible rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:isADAAccessible>
    <nycSubway:hasElevators rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:hasElevators>
    <nycSubway:acceptsFarePayment rdf:resource="http://vidyaastra.org/ontology/nycSubway#OMNYTap"/>
    <nycSubway:supportsAccessibility rdf:resource="http://vidyaastra.org/ontology/nycSubway#ElevatorStatus"/>
    <nycSubway:staffedBy rdf:resource="http://vidyaastra.org/ontology/nycSubway#StationAgent"/>
    <nycSubway:connectsTo rdf:resource="http://vidyaastra.org/ontology/nycSubway#Emergency911"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#PennStation34StPenn">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <nycSubway:entityName>Penn Station (34 St–Penn)</nycSubway:entityName>
    <nycSubway:description>Major intercity hub (LIRR/Amtrak/NJ Transit connections), OMNY readers, customer service nearby</nycSubway:description>
    <nycSubway:isADAAccessible rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:isADAAccessible>
    <nycSubway:hasElevators rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:hasElevators>
    <nycSubway:acceptsFarePayment rdf:resource="http://vidyaastra.org/ontology/nycSubway#OMNYTap"/>
    <nycSubway:staffedBy rdf:resource="http://vidyaastra.org/ontology/nycSubway#StationAgent"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#GrandCentral42St">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <nycSubway:entityName>Grand Central–42 St</nycSubway:entityName>
    <nycSubway:description>Major hub: Midtown east, OMNY readers, station agents, elevators (varies by entrance)</nycSubway:description>
    <nycSubway:isADAAccessible rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:isADAAccessible>
    <nycSubway:hasElevators rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:hasElevators>
    <nycSubway:acceptsFarePayment rdf:resource="http://vidyaastra.org/ontology/nycSubway#OMNYTap"/>
    <nycSubway:supportsAccessibility rdf:resource="http://vidyaastra.org/ontology/nycSubway#ElevatorStatus"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#FultonSt">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <nycSubway:entityName>Fulton St</nycSubway:entityName>
    <nycSubway:description>Lower Manhattan transfer hub with many elevators and complex corridors</nycSubway:description>
    <nycSubway:isADAAccessible rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:isADAAccessible>
    <nycSubway:hasElevators rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:hasElevators>
    <nycSubway:supportsAccessibility rdf:resource="http://vidyaastra.org/ontology/nycSubway#ElevatorStatus"/>
    <nycSubway:connectsTo rdf:resource="http://vidyaastra.org/ontology/nycSubway#Emergency911"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#UnionSq14St">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SubwayStation"/>
    <nycSubway:entityName>14 St–Union Sq</nycSubway:entityName>
    <nycSubway:description>Busy transfer station; partial elevator access at select entrances</nycSubway:description>
    <nycSubway:isADAAccessible rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</nycSubway:isADAAccessible>
    <nycSubway:hasElevators rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</nycSubway:hasElevators>
    <nycSubway:supportsAccessibility rdf:resource="http://vidyaastra.org/ontology/nycSubway#ElevatorStatus"/>
  </rdf:Description>

  <!-- Fare Services -->
  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#OMNYTap">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#FareService"/>
    <nycSubway:entityName>OMNY Tap</nycSubway:entityName>
    <nycSubway:description>OMNY contactless payment: tap with card / phone / OMNY card. Weekly fare cap available.</nycSubway:description>
    <nycSubway:providesService rdf:resource="http://vidyaastra.org/ontology/nycSubway#OMNYSystemBackend"/>
  </rdf:Description>

  <!-- Accessibility Services -->
  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#ElevatorStatus">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#AccessibilityService"/>
    <nycSubway:entityName>Elevator Status</nycSubway:entityName>
    <nycSubway:description>Elevator/escalator status & outages tracked by MTA. Check before travel.</nycSubway:description>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#ADAAssistance">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#AccessibilityService"/>
    <nycSubway:entityName>ADA Assistance / Access-A-Ride info</nycSubway:entityName>
    <nycSubway:description>Information on accessibility, reduced-fare, and paratransit services.</nycSubway:description>
  </rdf:Description>

  <!-- Support Services -->
  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#StationAgent">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
    <nycSubway:entityName>Station Agent / Enquiry</nycSubway:entityName>
    <nycSubway:description>On-site station staff for travel help, directions, and emergency assistance.</nycSubway:description>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#LostFound">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
    <nycSubway:entityName>Lost & Found</nycSubway:entityName>
    <nycSubway:description>MTA lost & found procedures and claims.</nycSubway:description>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#MTAServiceStatus">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#BackendSystem"/>
    <nycSubway:entityName>MTA Service Status</nycSubway:entityName>
    <nycSubway:description>Official service updates: delays, planned work, suspensions.</nycSubway:description>
    <nycSubway:providesService rdf:resource="http://vidyaastra.org/ontology/nycSubway#TimesSquare42St"/>
    <nycSubway:providesService rdf:resource="http://vidyaastra.org/ontology/nycSubway#PennStation34StPenn"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#OMNYSystemBackend">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#BackendSystem"/>
    <nycSubway:entityName>OMNY System Backend</nycSubway:entityName>
    <nycSubway:description>Contactless payment backend (OMNY) used across MTA network.</nycSubway:description>
    <nycSubway:providesService rdf:resource="http://vidyaastra.org/ontology/nycSubway#OMNYTap"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#MTA511Line">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
    <nycSubway:entityName>511NY / MTA Customer Line</nycSubway:entityName>
    <nycSubway:description>Dial 511 for MTA service info and customer assistance (automated + transfer options).</nycSubway:description>
    <nycSubway:supportPhoneNumber>511</nycSubway:supportPhoneNumber>
    <nycSubway:operatingHours>24/7</nycSubway:operatingHours>
    <nycSubway:canContactVia rdf:resource="http://vidyaastra.org/ontology/nycSubway#MTAServiceStatus"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#NYC311">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#SupportService"/>
    <nycSubway:entityName>311 NYC Non-Emergency</nycSubway:entityName>
    <nycSubway:description>311 for non-emergency city assistance; 911 for emergencies.</nycSubway:description>
    <nycSubway:supportPhoneNumber>311</nycSubway:supportPhoneNumber>
    <nycSubway:operatingHours>24/7</nycSubway:operatingHours>
    <nycSubway:canContactVia rdf:resource="http://vidyaastra.org/ontology/nycSubway#LostFound"/>
  </rdf:Description>

  <!-- Emergency Services -->
  <rdf:Description rdf:about="http://vidyaastra.org/ontology/nycSubway#Emergency911">
    <rdf:type rdf:resource="http://vidyaastra.org/ontology/nycSubway#EmergencyService"/>
    <nycSubway:entityName>Emergency (911)</nycSubway:entityName>
    <nycSubway:description>Call 911 for immediate life-safety emergencies in stations or trains.</nycSubway:description>
    <nycSubway:supportPhoneNumber>911</nycSubway:supportPhoneNumber>
    <nycSubway:operatingHours>24/7</nycSubway:operatingHours>
  </rdf:Description>

</rdf:RDF>

